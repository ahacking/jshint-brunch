{
  "name": "jshint-brunch",
  "version": "1.5.1",
  "description": "Adds JSHint support to brunch.",
  "author": "Paul Miller (http://paulmillr.com/)",
  "homepage": "https://github.com/brunch/jshint-brunch",
  "repository": {
    "type": "git",
    "url": "git@github.com:brunch/jshint-brunch.git"
  },
  "main": "./lib/index",
  "scripts": {
